<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.expoyazilim.kitapligim.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/bookshelflauncher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/bookshelflauncher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BookAddActivity"
            android:label="@string/kitap_ekle"/>
        <activity android:name=".BookShelfAddActivity"
            android:label="@string/kitaplik_ekle"></activity>
    </application>

</manifest>